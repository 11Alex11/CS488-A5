OBJECTS := \
	$(OBJDIR)/Mesh.o \
	$(OBJDIR)/Skybox.o \
	$(OBJDIR)/Sound.o \
	$(OBJDIR)/Object.o \
	$(OBJDIR)/A5.o \
	$(OBJDIR)/Main.o \
